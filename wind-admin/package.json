    "script-loader": "^0.7.2",
    "serve-static": "^1.14.1",
    "svg-sprite-loader": "^6.0.9",
    "svgo": "^2.3.1",
    "vue-template-compiler": "^2.6.14",
    "validator": "^13.6.0"
  },
  "engines": {
    "node": ">=14.0",
    "npm": ">= 6.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
